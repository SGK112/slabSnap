# ============================================
# REMODELY.AI Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!
# ============================================

# ============================================
# VIBECODE SERVICES (for AI-powered features)
# ============================================
EXPO_PUBLIC_VIBECODE_PROJECT_ID=your-vibecode-project-id
EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY=sk-proj-your-openai-api-key-here
EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY=sk-ant-api03-your-anthropic-key-here
EXPO_PUBLIC_VIBECODE_GROK_API_KEY=xai-your-grok-api-key-here
EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY=your-google-ai-api-key-here
EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# ============================================
# GOOGLE MAPS (required for location features)
# ============================================
# Get your key at: https://console.cloud.google.com/google/maps-apis
# Enable: Maps SDK for iOS, Maps SDK for Android, Places API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=AIza...your-google-maps-key

# ============================================
# STRIPE (required for payments)
# ============================================
# Get your keys at: https://dashboard.stripe.com/apikeys
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key

# ============================================
# BACKEND API (if using a backend server)
# ============================================
EXPO_PUBLIC_API_URL=https://your-api-server.com/api
EXPO_PUBLIC_API_VERSION=v1

# ============================================
# FIREBASE (optional - for auth/database)
# ============================================
# EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
# EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
# EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
# EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
# EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef

# ============================================
# SUPABASE (optional - alternative to Firebase)
# ============================================
# EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# ============================================
# APP CONFIGURATION
# ============================================
EXPO_PUBLIC_APP_ENV=development
# Options: development, staging, production
